			function doOnLoad(result, userurl, redirurl, adminurl, timeleft) {
				if ((result === 1) || (result === 4)) {	// success or already
					var url;
					if (adminurl !== '') {
						url = adminurl;
					} else if (redirurl !== '') {
						url = redirurl;
					} else if (userurl !== '') {
						url = userurl;
					}

					if (typeof url !== 'undefined') {
						var win = window.open('https://alcasar.laplateforme.io/status.php', '_blank');

						if ((win === null) || (typeof win === 'undefined')) { // Pop-up blocked
							window.location = 'https://alcasar.laplateforme.io/status.php';
						} else {
							window.location = url;
						}
					} else {
						window.location = 'https://alcasar.laplateforme.io/status.php';
					}
				}
				if ((result === 2) || (result === 3) || result === 5) { // failed or logoff or notyet
					// document.form1.username.focus();
				}
			}
